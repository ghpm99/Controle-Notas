    <stylesheets>
        <URL value="@controle.css" />
    </stylesheets>
   <children>
      <ChoiceBox fx:id="campos" layoutX="14.0" layoutY="14.0" prefWidth="150.0" />
      <TextField fx:id="campo" layoutX="194.0" layoutY="14.0" onKeyPressed="#buscarEnter" prefHeight="25.0" prefWidth="324.0" />
      <Button layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#buscar" text="Buscar" />
      <TableView fx:id="fornecedores" layoutX="14.0" layoutY="47.0" prefHeight="305.0" prefWidth="574.0">
        <columns>
          <TableColumn fx:id="cnpj" prefWidth="200.0" text="CNPJ" />
